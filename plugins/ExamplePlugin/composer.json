{
    "name": "yfsns/example-plugin",
    "description": "示例插件，演示如何正确开发安全合规的插件",
    "version": "1.0.0",
    "type": "yfsns-plugin",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "YFSNS Team",
            "email": "team@yfsns.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=10.0"
